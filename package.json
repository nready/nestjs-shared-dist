    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.17",
    "@nestjs/typeorm": "^10.0.2",
    "@types/amqplib": "^0.10.6",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
