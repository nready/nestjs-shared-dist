    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.4.17",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "10.4.17",
    "@nestjs/swagger": "^8.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "@nready/shared": "file:",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
